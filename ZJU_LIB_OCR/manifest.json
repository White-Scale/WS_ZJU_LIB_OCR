{
	"manifest_version": 2,
	"name": "浙大图书馆验证码识别",
	"version": "1.0",
	"description": "简单的浙大图书馆登录验证码识别,大一新生的没事创造",
	"author": "2023White_Scale",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-1.8.3.js","content-script.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": 
	[
		"inject.js"
	]
}